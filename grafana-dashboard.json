{
  "dashboard": {
    "id": null,
    "title": "Coffee Shop Monitoring Dashboard",
    "tags": ["coffee-shop", "monitoring"],
    "style": "dark",
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "Requests Per Second (RPS)",
        "type": "stat",
        "targets": [
          {
            "expr": "rate(http_server_requests_seconds_count[5m])",
            "refId": "A",
            "legendFormat": "RPS"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "reqps",
            "color": {
              "mode": "palette-classic"
            }
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 0
        }
      },
      {
        "id": 2,
        "title": "Average Response Time",
        "type": "stat",
        "targets": [
          {
            "expr": "rate(http_server_requests_seconds_sum[5m]) / rate(http_server_requests_seconds_count[5m])",
            "refId": "A",
            "legendFormat": "Avg Response Time"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "s",
            "color": {
              "mode": "palette-classic"
            }
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 0
        }
      },
      {
        "id": 3,
        "title": "Coffee Orders Created",
        "type": "stat",
        "targets": [
          {
            "expr": "coffee_orders_created_total",
            "refId": "A",
            "legendFormat": "Orders Created"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "color": {
              "mode": "palette-classic"
            }
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 8
        }
      },
      {
        "id": 4,
        "title": "Coffee Orders Delivered",
        "type": "stat",
        "targets": [
          {
            "expr": "coffee_orders_delivered_total",
            "refId": "A",
            "legendFormat": "Orders Delivered"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "color": {
              "mode": "palette-classic"
            }
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 8
        }
      },
      {
        "id": 5,
        "title": "JVM Memory Used",
        "type": "stat",
        "targets": [
          {
            "expr": "jvm_memory_used_bytes{area=\"heap\"}",
            "refId": "A",
            "legendFormat": "Heap Memory Used"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "bytes",
            "color": {
              "mode": "palette-classic"
            }
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 16
        }
      },
      {
        "id": 6,
        "title": "JVM Memory Non-Heap Used",
        "type": "stat",
        "targets": [
          {
            "expr": "jvm_memory_used_bytes{area=\"nonheap\"}",
            "refId": "A",
            "legendFormat": "Non-Heap Memory Used"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "bytes",
            "color": {
              "mode": "palette-classic"
            }
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 16
        }
      },
      {
        "id": 7,
        "title": "RPS Over Time",
        "type": "timeseries",
        "targets": [
          {
            "expr": "rate(http_server_requests_seconds_count[1m])",
            "refId": "A",
            "legendFormat": "RPS"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "reqps",
            "color": {
              "mode": "palette-classic"
            }
          }
        },
        "gridPos": {
          "h": 8,
          "w": 24,
          "x": 0,
          "y": 24
        }
      },
      {
        "id": 8,
        "title": "Response Time Percentiles",
        "type": "timeseries",
        "targets": [
          {
            "expr": "histogram_quantile(0.50, rate(http_server_requests_seconds_bucket[5m]))",
            "refId": "A",
            "legendFormat": "50th percentile"
          },
          {
            "expr": "histogram_quantile(0.95, rate(http_server_requests_seconds_bucket[5m]))",
            "refId": "B",
            "legendFormat": "95th percentile"
          },
          {
            "expr": "histogram_quantile(0.99, rate(http_server_requests_seconds_bucket[5m]))",
            "refId": "C",
            "legendFormat": "99th percentile"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "s",
            "color": {
              "mode": "palette-classic"
            }
          }
        },
        "gridPos": {
          "h": 8,
          "w": 24,
          "x": 0,
          "y": 32
        }
      },
      {
        "id": 9,
        "title": "Orders Created Over Time",
        "type": "timeseries",
        "targets": [
          {
            "expr": "coffee_orders_created_total",
            "refId": "A",
            "legendFormat": "Orders Created"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "color": {
              "mode": "palette-classic"
            }
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 40
        }
      },
      {
        "id": 10,
        "title": "Orders Delivered Over Time",
        "type": "timeseries",
        "targets": [
          {
            "expr": "coffee_orders_delivered_total",
            "refId": "A",
            "legendFormat": "Orders Delivered"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "color": {
              "mode": "palette-classic"
            }
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 40
        }
      },
      {
        "id": 11,
        "title": "JVM Memory Usage Over Time",
        "type": "timeseries",
        "targets": [
          {
            "expr": "jvm_memory_used_bytes{area=\"heap\"}",
            "refId": "A",
            "legendFormat": "Heap Memory"
          },
          {
            "expr": "jvm_memory_used_bytes{area=\"nonheap\"}",
            "refId": "B",
            "legendFormat": "Non-Heap Memory"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "bytes",
            "color": {
              "mode": "palette-classic"
            }
          }
        },
        "gridPos": {
          "h": 8,
          "w": 24,
          "x": 0,
          "y": 48
        }
      }
    ],
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "refresh": "5s"
  }
}
